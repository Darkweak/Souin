{
    experimental_http3
    order souin_cache before rewrite
    souin_cache {
        headers Content-Type Authorization
        olric {
            url `olric:3320`
        }
        ttl 1000
    }
}

:80

souin_cache {
    route /testing-purpose {
        headers Content-Type
    }
    route /testing-purpose-2 {
        headers Content-Type
        ttl 100
    }
    route /a* {
        ttl 20
    }
    route /a* {
        headers Content-Type
    }
    route * {
        headers Authorization
        ttl 666
    }
}
